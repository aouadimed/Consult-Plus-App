<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="110dp"
    android:layout_height="wrap_content"
    android:layout_marginVertical="6.50dp"
    android:layout_marginHorizontal="6.50dp"
    android:background="@drawable/radius_box"
    android:paddingHorizontal="10.40dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/time"
            android:layout_width="wrap_content"
            android:layout_height="19.50dp"
            android:layout_marginVertical="10.40dp"
            android:fontFamily="@font/poppins_medium"
            android:text="10:00"
            android:textColor="@color/black"
            android:textSize="14.30dp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="19.50dp"
            android:layout_marginVertical="10.40dp"
            android:fontFamily="@font/poppins_medium"
            android:text=" - "
            android:textColor="@color/black"
            android:textSize="14.30dp" />
        <TextView
            android:id="@+id/end"
            android:layout_width="wrap_content"
            android:layout_height="19.50dp"
            android:layout_marginVertical="10.40dp"
            android:fontFamily="@font/poppins_medium"
            android:text="11:00"
            android:textColor="@color/black"
            android:textSize="14.30dp" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>